<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pdir.screens.Screen2">

    <children>
        <fx:include fx:id="progHB" source="progressbar.fxml" />

        <HBox alignment="CENTER" fillHeight="false" VBox.vgrow="ALWAYS">
            <children>
                <Label alignment="CENTER" contentDisplay="TEXT_ONLY" maxHeight="120.0" minHeight="-Infinity" prefHeight="120.0" prefWidth="500.0" text="Mark images that should be immediately deleted from the study. You are free to mark any images you wish, but we specifically ask you to delete images with nudity, in locations where photography was prohibited, or of people who had asked not to be recorded." textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
                </Label>
            </children>
            <VBox.margin>
                <Insets bottom="5.0" left="30.0" right="30.0" top="5.0" />
            </VBox.margin>
        </HBox>
        <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="500.0" VBox.vgrow="ALWAYS">
            <content>
                <FlowPane id="flowPane" fx:id="photoFlow" alignment="CENTER" hgap="5.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" vgap="5.0" />
            </content>
        </ScrollPane>

        <fx:include fx:id="nextButton" source="nextbutton.fxml" />

    </children>
</VBox>
