<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pdir.ProgressBarController">
   <children>
      <Label fx:id="topLabel" text="Label">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" styleClass="progLabel">
            <children>
                <Label styleClass="progLabel" text="Start">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
                <ProgressBar fx:id="progressBar" minHeight="50.0" minWidth="500.0" prefWidth="200.0" progress="0.0" />
                <Label styleClass="progLabel" text="End">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
   </children>
</VBox>
