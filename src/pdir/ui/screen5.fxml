<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pdir.screens.Screen5">
   <children>
      <fx:include fx:id="progHB" source="progressbar.fxml" />
      <ScrollPane fitToWidth="true" hbarPolicy="NEVER" prefHeight="500.0" VBox.vgrow="ALWAYS">
         <content>
            <VBox alignment="CENTER" spacing="10.0">
               <children>
                  <Label alignment="CENTER" styleClass="question-label" text="You pressed the delete button on the camera at the following times." textAlignment="CENTER" wrapText="true" />
                  <Label alignment="CENTER" contentDisplay="CENTER" minHeight="50.0" minWidth="600.0" text="Why did you use the delete button on the camera on..." textAlignment="CENTER" wrapText="true" />
                  <VBox fx:id="deleteEventVbox" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </VBox>
               </children>
            </VBox>
         </content>
      </ScrollPane>
      <fx:include fx:id="nextButton" source="nextbutton.fxml" />
   </children>
</VBox>
